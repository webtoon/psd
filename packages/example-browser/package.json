{
  "name": "@webtoon/psd-example-browser",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wireit",
    "build:psd": "wireit",
    "start": "wireit",
    "start:vite": "vite"
  },
  "dependencies": {
    "@webtoon/psd": "*"
  },
  "devDependencies": {
    "npm-run-all2": "^6.0.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.14",
    "wireit": "^0.7.2"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "dependencies": [
        "build:psd"
      ]
    },
    "build:psd": {
      "dependencies": [
        "../psd:build"
      ]
    },
    "start": {
      "command": "run-p 'build:psd --watch' start:vite",
      "dependencies": [
        "build:psd"
      ]
    }
  }
}
