{
  "name": "@webtoon/psd-example-browser",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wireit",
    "start": "wireit"
  },
  "dependencies": {
    "@webtoon/psd": "*"
  },
  "devDependencies": {
    "npm-run-all2": "^6.0.2",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "wireit": "^0.9.5"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "dependencies": [
        "../psd:build"
      ]
    },
    "start": {
      "command": "vite",
      "service": true,
      "dependencies": [
        {
          "script": "../psd:build",
          "cascade": false
        }
      ]
    }
  }
}
