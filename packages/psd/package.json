{
  "name": "@webtoon/psd",
  "version": "0.2.0",
  "description": "lightweight psd file parser with no dependency written in typescript",
  "homepage": "https://webtoon.github.io/psd",
  "license": "MIT",
  "author": "W FE Tech <dl_wfetech@webtoonscorp.com>",
  "contributors": [
    "Lee Dong Hyun <moving.hyun@webtoonscorp.com>",
    "Kang Ye Hyoung <yehyoung.kang@webtoonscorp.com>"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "wireit",
    "prepublishOnly": "npm run build",
    "release": "wireit",
    "test": "wireit"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@webtoon/psd-decoder": "*",
    "release-it": "^15.1.1",
    "typescript": "^4.7.3",
    "vite": "^2.9.14",
    "vitest": "^0.18.0",
    "wireit": "^0.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "clean": false,
      "dependencies": [
        "../decoder:build"
      ],
      "files": [
        "src",
        "tsconfig.json",
        "vite.config.ts"
      ],
      "output": [
        "dist"
      ]
    },
    "release": {
      "command": "release-it",
      "dependencies": [
        "build"
      ]
    },
    "test": {
      "command": "vitest run",
      "clean": false,
      "dependencies": [
        "../decoder:build"
      ],
      "files": [
        "src",
        "tests",
        "tsconfig.json",
        "vite.config.ts"
      ]
    }
  }
}
