{
  "name": "@webtoon/psd",
  "version": "0.3.0",
  "description": "lightweight psd file parser with no dependency written in typescript",
  "homepage": "https://webtoon.github.io/psd",
  "license": "MIT",
  "author": "W FE Tech <dl_wfetech@webtoonscorp.com>",
  "contributors": [
    "Lee Dong Hyun <moving.hyun@webtoonscorp.com>",
    "Kang Ye Hyoung <yehyoung.kang@webtoonscorp.com>"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "wireit",
    "prepublishOnly": "npm run build",
    "release": "release-it",
    "test": "wireit"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@rollup/plugin-typescript": "^9.0.1",
    "@webtoon/psd-decoder": "*",
    "release-it": "^15.5.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "wireit": "^0.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "clean": false,
      "dependencies": [
        "../decoder:build"
      ],
      "files": [
        "src",
        "tsconfig.json",
        "vite.config.ts"
      ],
      "output": [
        "dist"
      ]
    },
    "test": {
      "command": "vitest run",
      "clean": false,
      "dependencies": [
        "../decoder:build"
      ],
      "files": [
        "src",
        "tests",
        "tsconfig.json",
        "vite.config.ts"
      ]
    }
  }
}
