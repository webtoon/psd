{
  "name": "@webtoon/psd-benchmark",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wireit",
    "start": "wireit"
  },
  "dependencies": {
    "@webtoon/psd": "*",
    "ag-psd": "^15.3.0",
    "chart.js": "^4.3.0"
  },
  "devDependencies": {
    "@types/psd": "^3.4.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "wireit": "^0.9.5"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "dependencies": [
        "../psd:build"
      ]
    },
    "start": {
      "command": "vite",
      "service": true,
      "dependencies": [
        {
          "script": "../psd:build",
          "cascade": false
        }
      ]
    }
  }
}
