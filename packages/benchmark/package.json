{
  "name": "@webtoon/psd-benchmark",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wireit",
    "start": "wireit"
  },
  "dependencies": {
    "@webtoon/psd": "*",
    "ag-psd": "^15.0.0",
    "chart.js": "^4.2.1"
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "wireit": "^0.9.5"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "dependencies": [
        "../psd:build"
      ]
    },
    "start": {
      "command": "vite",
      "service": true,
      "dependencies": [
        {
          "script": "../psd:build",
          "cascade": false
        }
      ]
    }
  }
}
